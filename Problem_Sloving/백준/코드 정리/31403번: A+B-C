** 주의 사항 **
1. std::stoi 함수에 대해 이해하기 


int main()
{
    std::ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);
    
    string strA, strB, strC;
    cin >> strA >> strB >> strC;
 
    cout << std::stoi(strA) + std::stoi(strB) - std::stoi(strC) << "\n";
    cout << std::stoi(strA + strB) - std::stoi(strC);

    return 0;
}
